{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "banquet-cli test",
            "program": "${workspaceFolder}/node_modules/.bin/banquet",
            "args": [
                "test"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "banquet-cli start",
            "program": "${workspaceFolder}/node_modules/.bin/banquet",
            "args": [
                "start"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "banquet-cli build",
            "program": "${workspaceFolder}/node_modules/.bin/banquet",
            "args": [
                "build"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "banquet-cli build",
            "program": "${workspaceFolder}/node_modules/.bin/banquet",
            "args": [
                "build"
            ]
        }
    ]
}